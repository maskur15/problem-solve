///start try success
//2d vector
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n = 10;
    std::vector<std::vector<int>> normal;
    normal.resize(n);   //Allocating 'n' empty vectors
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < 20; j++)
        {
             normal[i].push_back(j);
        }
    }
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<normal[i].size();j++)
        {
            cout << normal[i][j] << "  ";
        }
        cout << endl;
    }
    return 0;
}
