////recursion
#include<bits/stdc++.h>
using namespace std;
int fact(int n)
{
    if(n==1)
    {
         cout << n << endl;
    }
    else
    {
        cout << n << endl;
        fact(n-1);
        cout << "Reverse" << endl;
        cout << n << endl;
    }
}
int main()
{
    int n;
    cin >> n;
    fact(n);
}
